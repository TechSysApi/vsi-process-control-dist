{
  "namespace": "ReadRates",
  "name": "main",
  "type": "record",
  "doc": "Contains an array of read rates.",
  "fields": [
    {
      "name": "timestamp",
      "doc": "The date+timestamp which identifies the point in time, where the information was generated, including milliseconds.",
      "type": "string"
    },
    {
      "name": "locationKey",
      "doc": "[ACTION] Attribute is required in future releases after WESort is rolled out in all sorting centers.\nUnique identifier of the sorting center (ONRP).",
      "default": null,
      "type": [
        "null",
        "string"
      ]
    },
    {
      "name": "readRates",
      "doc": "",
      "type": [
        "null",
        {
          "type": "array",
          "items": {
            "type": "record",
            "name": "ReadRate",
            "doc": "The read rate event of an element. The counter will be used for visualization and statistic purposes.",
